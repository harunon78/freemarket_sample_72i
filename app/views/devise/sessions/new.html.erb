<div class ="main-contents" >
  <div class ="main-contents__header">
  </div>
  <div class ="main-contents__form">
    <div class ="main-contents__form__top">
      <h3>ログイン</h3>
    </div>
      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class ="main-contents__form__main">
      <div class ="main-contents__form__main__inner">
        <div class="main-contents__form__main__inner__field">
          <%= f.label :email, "メールアドレス" %><br />
          <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
        </div>
        <div class="main-contents__form__main__inner__field">
          <%= f.label :password, "パスワード" %><br />
          <%= f.password_field :password, autocomplete: "current-password" %>
        </div>
        <% if devise_mapping.rememberable? %>
          <div class="field">
            <%= f.check_box :remember_me %>
            <%= f.label :remember_me, "次回から入力を省略" %>
          </div>
        <% end %>
        <div class="main-contents__form__main__inner__actions">
          <%= f.submit "ログイン" %>
        </div>
      <% end %>
      <%= render "devise/shared/links" %>
      </div>
    </div>
  </div>
  <div class ="main-contents__fotter">
  </div>
</div>
